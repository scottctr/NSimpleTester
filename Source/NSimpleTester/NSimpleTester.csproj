<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	  <AnalysisLevel>5.0</AnalysisLevel>
	  <LangVersion>latest</LangVersion>
	  <Copyright>Copyright © 2019</Copyright>
    <Version>1.2.3</Version>
    <Authors>Scott L Carter</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/scottctr/NSimpleTester</PackageProjectUrl>
    <RepositoryUrl>https://github.com/scottctr/NSimpleTester</RepositoryUrl>
    <PackageTags>.net unit testing</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.3.1" />
    <PackageReference Include="System.CodeDom" Version="4.6.0" />
    <PackageReference Include="System.Runtime.Loader" Version="4.3.0" />
  </ItemGroup>

</Project>
